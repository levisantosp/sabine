{
  "name": "sabine",
  "version": "2.9.0",
  "description": "your esports coverage discord bot",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "node --env-file=.env test.ts",
    "start": "node --env-file=.env .",
    "dev": "node --watch --env-file=.env src/index.ts",
    "build": "pnpm lint && rm -rf dist && tsc",
