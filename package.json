    "humanize-duration": "^3.33.1",
    "moment": "^2.30.1",
    "oceanic.js": "^1.12.0",
    "players": "github:levisantosp/sabine-players",
    "redis": "^5.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
